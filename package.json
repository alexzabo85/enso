{
  "name": "restful-service",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alex639hz/enso.git"
  },
  "scripts": {
    "fs-server": "node ./server/write",
    "redis": "redis-server",
    "dev": "clear && concurrently --kill-others \"npm run fs-server\" \"npm run server\"",
    "server": "nodemon",
    "start": "set NODE_ENV=production &&  node ./server/write &&  node ./bin/www",
    "kill": "kill $(lsof -t -i:8080)",
    "test": "clear && ./node_modules/jest/bin/jest.js ./tests/main.test.js --forceExit  --detectOpenHandles"
  },
  "dependencies": {
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "concurrently": "^6.2.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "express-ws": "^4.0.0",
    "formidable": "^1.2.2",
    "helmet": "^4.2.0",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.6",
    "morgan": "~1.9.1",
    "request": "^2.88.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "./server"
    ],
    "delay": 2,
    "exec": "node ./server/bin/www"
  }
}